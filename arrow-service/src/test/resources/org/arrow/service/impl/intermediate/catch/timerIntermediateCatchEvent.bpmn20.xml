<definitions xmlns="http://www.omg.org/spec/BPMN/20100524/MODEL"
             typeLanguage="http://www.w3.org/2001/XMLSchema"
             expressionLanguage="http://www.w3.org/1999/XPath" targetNamespace="http://www.arrow.org/test">

    <process id="timerIntermediateCatchEventTest" isExecutable="true">
        <startEvent id="startevent1" name="Start"/>
        <sequenceFlow id="flow1" targetRef="throw1" sourceRef="startevent1"/>
        <intermediateCatchEvent id="throw1">
            <timerEventDefinition>
                <timeDuration>PT1S</timeDuration>
            </timerEventDefinition>
        </intermediateCatchEvent>
        <sequenceFlow id="flow2" targetRef="endevent1" sourceRef="throw1"/>
        <endEvent id="endevent1" name="End"/>
    </process>

</definitions>